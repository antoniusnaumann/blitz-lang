fn main() Void {
    let a = 'a'
    let b = 'b'
    
    let eq_result = switch a == a {
        true { "equal" }
        false { "not equal" }
        _ { "error" }
    }
    
    let ne_result = switch a != b {
        true { "not equal" }
        false { "equal" }
        _ { "error" }
    }
    
    let gt_result = switch b > a {
        true { "b greater than a" }
        false { "b not greater" }
        _ { "error" }
    }
    
    let lt_result = switch a < b {
        true { "a less than b" }
        false { "a not less" }
        _ { "error" }
    }
    
    let ge_result = switch b >= a {
        true { "b greater or equal" }
        false { "b not greater or equal" }
        _ { "error" }
    }
    
    let le_result = switch a <= b {
        true { "a less or equal" }
        false { "a not less or equal" }
        _ { "error" }
    }
}
